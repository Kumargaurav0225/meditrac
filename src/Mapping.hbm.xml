<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="proj.meditrac.entities.Member" table="SYSTEM_MEMBERS">

		<id name="id" column="SYS_ID" />
		<property name="password" column="PASS_ENC" not-null="true" length="32" />
		<property name="group" column="USER_GROUP" not-null="true" length="16"/>
		<property name="active" column="IS_ACTIVE" not-null="true" length="10"/>
		<property name="createdAt" column="CREATED_AT" not-null="true"/>
		<property name="lastLogin" column="LAST_LOGIN" />
		
	</class>

</hibernate-mapping>